package p2023_07_19;

public class Report {
// 구구단 2차원 배열

	public static void main(String[] args) {

		int guGu[][] = new int[9][8];

		for (int i = 0; i <= 7; i++) {
			for (int j = 0; j <= 8; j++) {
				guGu[i][j] =(i + 3) * (j + 1);
				System.out.print(guGu[i][j]+"\t");
			}
		}
		System.out.println(guGu[5][3]);

	}
}
