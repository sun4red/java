package p2023_07_14;

public class For01 {

	public static void main(String[] args) {

		// for 문
		// for ( 초기값; 조건식; 증감식) {
		// 반복 실행할 문장;
		// }

		// 사랑해요 메세지를 10번 출력하세요!

		// 실행되는 문장이 1줄이면 중괄호 생략 가능
		for (int i = 1; i <= 10; i++) {
			System.out.println(i + "사랑해요");
		}
		System.out.println("종료");
	}
}
