package p2023_07_13;

import java.util.Scanner;

public class Oper07 {
	public static void main(String[] args) {

		// 논리 연산자 : ||, &&, !
		
		int chap1, chap2, chap3, chap4, chap5;
		
		System.out.println("5과목 점수 순서대로 입력");
		
		Scanner sc = new Scanner(System.in);
		
		chap1 = sc.nextInt();
		chap2 = sc.nextInt();
		chap3 = sc.nextInt();
		chap4 = sc.nextInt();
		chap5 = sc.nextInt();
		
		if (((chap1+chap2+chap3+chap4+chap5)/5)>=60) {
			System.out.println("평균점수 : " + (chap1+chap2+chap3+chap4+chap5)/5);
		}
		
		
	}
}
