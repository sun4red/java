package p2023_07_19;

public class Report {
// 구구단 2차원 배열

	public static void main(String[] args) {

		int guGu[][] = new int[9][8];

		for (int c = 0; c <= 7; c++) {
			for (int r = 0; r <= 8; r++) {
				guGu[c][j] =(c + 1) * (r + 1);
				System.out.println(guGu[c][r]+"\t");
			}
		}
		System.out.println(guGu[5][3]);

	}
}
