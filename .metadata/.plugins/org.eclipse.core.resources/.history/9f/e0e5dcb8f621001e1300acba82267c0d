package p2023_07_14;

public class Switch01 {
	public static void main(String[] args) {

		// switch ~ case 문

		int s = 60;

		switch (s) {
		case 90:
			System.out.println("A학점");
			break;
		case 80:
			System.out.println("B학점");
			break;
		case 70:
			System.out.println("C학점");
			break;
		case 60:
			System.out.println("D학점");
			break;
		default:
			System.out.println("F학점");
		}
	}
}
