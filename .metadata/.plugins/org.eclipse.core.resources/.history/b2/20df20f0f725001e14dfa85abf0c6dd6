package p2023_07_19;

class MyDate05 {
	private int year;
	private int month;
	private int day;

	public MyDate05() { // 기본 생성자
  }


	public MyDate05(int new_year, int new_month, int new_day){  
    year= new_year;
    month=new_month;
    day=new_day;
  }

	public void print() {
		System.out.println(year + "/" + month + "/" + day);
	}
}
// MyDate end

public class ConstructorTest03 {
	public static void main(String[] args) {
		MyDate05 d = new MyDate05();
		d.print();

		MyDate05 d2 = new MyDate05(2023, 7, 19);
		d2.print();

		// privaate 접근제어자는 외부 클래스의 접근을 허용하지 않기 때문에
		// 집접 접근할 수 ㅇ벗ㅂ다.
	}
}