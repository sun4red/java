package p2023_07_21;

public class MathEx {

	public static void main(String[] args) {
		
		
		
		// Math 클래스
		// Math 클래스 = 정적 필드 + 정적 메소드
		
		// Math 클래스는 생성자가 제공되지 않기 때문에, Math 클래스로 직접 객체 생성 불가
//		Math m = new Math(); // 오류 발생
		
		
		System.out.println("E="+Math.E); // 오일러 상수
		System.out.println("PI="+Math.PI); // 원주율
		
		
		
		
	}
	
}
