package p2023_07_19;

public class Report {
// 구구단 2차원 배열

	public static void main(String[] args) {

		int guGu[][] = new int[9][8];

		for (int r = 0; r <= 7; r++) {
			for (int c = 0; c <= 8; c++) {
				guGu[r][c] =(c + 1) * (r + 2);
				System.out.print(guGu[r][c]+"\t");
			}
			System.out.println();
		}
		System.out.println(guGu[5][3]);

	}
}
