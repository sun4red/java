package p2023_07_13;

public class CastingEx01 {
	public static void main(String[] args) {

		// 서로다른 자료형의 변수를 산술연산을 수행하면 가장 큰 자료형으로 자동 형변환이 된다.

		byte value1 = 10;
		int value2 = 100;
		long value3 = 1000L;
		long result = value1 + value2 + value3;
		System.out.println(result);

		int i = 100;
		double d = 3.14;
		double result2 = i + d;
		System.out.println(result2);
		

	}
}
