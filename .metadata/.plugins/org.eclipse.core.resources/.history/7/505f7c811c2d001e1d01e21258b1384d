package p2023_07_28;

public class Re {

	static void yoon(int year) {
		if (year % 4 == 0) {
			if (year % 100 == 0) {
				System.out.println("윤년");

			} else if (year % 400 == 0) {
				System.out.println("윤년이 아님");
			}
		} else {
			System.out.println("윤년이 아님");
		}
	}

	public static void main(String[] args) {
		int year = 2000;
		yoon(year);
	}

}
