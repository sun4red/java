package p2023_07_26;

public class Report {

	public static void main(String[] args) {

		int r6[] = new int[6];

		for (int ran = 0; ran < 6; ran++) {
			r6[ran] = (int) (Math.random() * 45 + 1);
			if (ran > 1) {
				for (int chk = 0; chk < ran; chk++) {
					if (r6[chk] == r6[ran]) {
						ran--;
					}
				}

			}

		}

		for (int p = 0; p < 6; p++) {
			System.out.println("번호" + (p + 1) + " : " + r6[p]);
		}

	}

}
