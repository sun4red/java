package p2023_07_17;

import java.util.Scanner;

public class Report {

	static int f(int n) {

		int result = 1;
		for (int i = n;; i--) {
			result *= i;
			if (i == 1) {
				break;
			}
		}
		return result;

	}

	public static void main(String[] args) {

		System.out.println("정수를 입력하세요");
		
		Scanner sc = new Scanner(System.in);
		
		int n1 = sc.nextInt();
		
		System.out.println("팩토리얼 값 :"+f(n1));

	}
}
